{
  "name": "desafio-native-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "npx -y tsc",
    "prestart": "npm run prisma:migrate && npm run build",
    "start": "node .",
    "predev": "npm run prisma:reset",
    "dev": "ts-node-dev src --watch src/app",
    "prisma:migrate": "npx prisma db push && npx prisma generate",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:seed": "npx prisma db seed",
    "compose:up": "(docker-compose up -d --build)",
    "compose:down": "(docker-compose down --remove-orphans)",
    "compose:reset": "npm run compose:down && npm run compose:up",
    "compose:up:db": "(docker-compose up -d --build db --wait)",
    "test": "npm run test:unit && npm run test:integration:docker",
    "test:unit": "jest ./src/test/unit",
    "pretest:integration": "dotenv -e .env.test npm run compose:up:db && dotenv -e .env.test npm run prisma:reset",
    "test:integration": "dotenv -e .env.test jest ./src/test/integration",
    "pretest:integration:docker": "dotenv -e .env.test npm run prisma:reset",
    "test:integration:docker": "npm run test:integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@jest-mock/express": "^2.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "express-async-errors": "^3.1.1",
    "jest": "^29.1.1",
    "jest-mock-extended": "^3.0.1",
    "joi": "^17.6.2",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "ws": "^8.9.0"
  }
}
