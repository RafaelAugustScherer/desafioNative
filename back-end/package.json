{
  "name": "desafio-native-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "npx -y tsc",
    "prestart": "npm run prisma:migrate && npm run build",
    "start": "node .",
    "predev": "npm run prisma:migrate",
    "dev": "ts-node-dev src --watch src/app",
    "prisma:migrate": "npx prisma db push && npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "compose:up": "(docker-compose up -d --build)",
    "compose:down": "(docker-compose down --remove-orphans)",
    "compose:reset": "npm run compose:down && npm run compose:up",
    "test": "jest -i ./src/test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@jest-mock/express": "^2.0.1",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^29.1.1",
    "jest-mock-extended": "^3.0.1",
    "prisma": "^4.4.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0"
  }
}
